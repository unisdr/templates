                    <use href="assets/icons/chevron-right.svg#chevron-right"></use>
                  </svg>
                </a>
              </li>
            </ul>
            <div class="dts-form-component">
              <select>
                <option>10 / page</option>
                <option>20 / page</option>
                <option>30 / page</option>
                <option>40 / page</option>
                <option>50 / page</option>
              </select>
            </div>
          </nav>
