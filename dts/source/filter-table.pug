<body>
  <div class="dts-page-container">

    include includes/_header.html

    <main class="dts-main-container">

      <div class="dts-page-header">
        include includes/_page-title.html
      </div>

      <div class="mg-container">

        include includes/_page-intro.html

        <form class="dts-form">
          <div class="dts-form__body">
            <div class="mg-grid mg-grid__col-3">
              <div class="dts-form-component">
                <label>
                  <div class="dts-form-component__label">
                    <span>Organisation</span>
                  </div>
                  <input type="text" placeholder="Type organisation name">
                </label>
              </div>
              <div class="dts-form-component">
                <label>
                  <div class="dts-form-component__label">
                    <span>Role</span>
                  </div>
                  <select>
                    <option value="all">All Roles</option>
                    <option value="data-viewer">Data Viewer</option>
                    <option value="data-collector">Data Collector</option>
                    <option value="data-validator">Data Validator</option>
                    <option value="admin">Admin</option>
                  </select>
              </label>
              </div>
            </div>
            <div class="dts-form__actions">
              <button type="button" class="mg-button mg-button--small mg-button-primary">Apply filters</button>
              <a href="#" class="mg-button mg-button--small mg-button-outline">Clear filters</a>
            </div>
          </div>
        </form>

        <section class="dts-page-section">
          <div class="dts-heading-4">400 Users found</div>
          <div class="dts-legend">
            <span class="dts-body-label">Status legend</span>
            <div class="dts-legend__item">
              <span class="dts-status dts-status--activated" aria-labelledby="legend7"></span>
              <span id="legend7">Account activated</span>
            </div>
            <div class="dts-legend__item">
              <span class="dts-status dts-status--pending" aria-labelledby="legend8"></span>
              <span id="legend8">Account activation pending</span>
            </div>
          </div>
          <table class="dts-table">
            <caption class="mg-u-sr-only">summary of the table's content</caption>
            <thead>
              <tr>
                <th>Status</th>
                //
                  Selected filter
                <th class="dts-table__filter dts-table__filter--active">Name</th>
                <th>Organisation</th>
                <th>Role</th>
                <th>Location</th>
                <th>Modified</th>
                <th>Edit</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>
                  <button type="button" class="dts-tooltip__button dts-status dts-status--activated" aria-labelledby="tooltip10"></button>
                  <div id="tooltip10" class="dts-tooltip--status" role="tooltip">
                    <span>Account activated</span>
                    <div class="dts-tooltip__arrow"></div>
                  </div>
                </td>
                <td>[First name] [last name]</td>
                <td>WMO</td>
                <td>Admin</td>
                <td>North Thailand, Chaing Mai</td>
                <td>31-10-2026</td>
                <td>
                  <button type="button" class="mg-button mg-button-table">
                    <svg aria-hidden="true" focusable="false" role="img">
                      <use href="assets/icons/edit.svg#edit"></use>
                    </svg>
                  </button>
                </td>
              </tr>
              <tr>
                <td>
                  <button type="button" class="dts-tooltip__button dts-status dts-status--pending" aria-labelledby="tooltip20"></button>
                  <div id="tooltip20" class="dts-tooltip--status" role="tooltip">
                    <span>Account activation pending</span>
                    <div class="dts-tooltip__arrow"></div>
                  </div>
                </td>
                <td>[First name] [last name]</td>
                <td>UNDP</td>
                <td>Role</td>
                <td>Level, sub-level</td>
                <td>31-10-2023</td>
                <td>
                  <button type="button" class="mg-button mg-button-table">
                    <svg aria-hidden="true" focusable="false" role="img">
                      <use href="assets/icons/edit.svg#edit"></use>
                    </svg>
                  </button>
                </td>
              </tr>
            </tbody>
          </table>
          <nav class="dts-pagination" role="navigation" aria-label="Pagination">
            <ul>
              <li>
                <a class="mg-button mg-button--small mg-button-ghost" href="#" aria-label="Previous page">
                  <svg aria-hidden="true" focusable="false" role="img">
                    <use href="assets/icons/chevron-left.svg#chevron-left"></use>
                  </svg>
                </a>
              </li>
              <li>
